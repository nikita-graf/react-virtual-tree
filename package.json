{
  "name": "react-virtual-tree",
  "version": "0.0.3",
  "description": "",
  "main": "es5/index.js",
  "jsnext:main": "es6/index.js",
  "files": [
    "css",
    "es5",
    "es6",
    "dist",
    "example",
    "*.md"
  ],
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --config webpack.config.js --content-base src --display-reasons --display-chunks --progress --hot --port 8888",
    "build": "npm run build-es5 && npm run build-es6 && npm run build-dist && npm run build-dist-min",
    "gh-pages-folder": "npm run build && mv dist gh-pages && cp -r example/* gh-pages && cp css/* gh-pages",
    "gh-pages": "npm run gh-pages-folder && git branch -f gh-pages && git checkout gh-pages && git add gh-pages && git commit -m 'upd' && git push origin :gh-pages && git subtree push --prefix gh-pages origin gh-pages && git checkout master",
    "build-es5": "rm -rf es5 && babel ./src -d es5",
    "build-es6": "rm -rf es6 && cp -r ./src ./es6",
    "build-dist": "webpack src/index.js dist/react-virtual-tree.js",
    "build-dist-min": "webpack -p src/index.js dist/react-virtual-tree.min.js --build",
    "publish": "npm run build"
  },
  "gh-pages-deploy": {
    "staticpath": "gh-pages-folder",
    "cname": "",
    "prep": [
      "gh-pages-folder"
    ],
    "post": [],
    "noprompt": false
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bem-cn": "^1.2.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
